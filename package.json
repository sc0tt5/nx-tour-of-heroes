  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "npm run dev:ssr",
    "serve": "nx serve --configuration=local --open --watch",
    "build": "nx build",
    "xtest": "npx jest --coverage",
    "test": "nx run-many --target=test --all --codeCoverage",
    "lint": "nx run-many --target=lint --all",
    "lint:styles": "stylelint \"{apps,libs}/**/*.scss\"",
    "doc:api": "compodoc -p apps/api/tsconfig.json -d tmp/documentation/api -s -o",
    "doc:tour-of-heroes": "compodoc -p apps/tour-of-heroes/tsconfig.json -d tmp/documentation/tour-of-heroes -s -o",
    "lighthouse": "lhci autorun",
    "analyze:bundles": "nx run tour-of-heroes:analyze --diffLoading=false",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:check": "nx update",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "serve:api": "nx serve api",
    "serve:database": "json-server --watch apps/tour-of-heroes/db.json --port 8800 --middlewares ./node_modules/json-server-reset",
    "dev:ssr": "concurrently --names 'JSONDB,NESTJS' --prefix-colors 'blue,yellow,' --kill-others \"npm:serve:database\" \"nx run tour-of-heroes:serve-ssr:local\" ",
    "build:ssr": "nx build --prod && nx run tour-of-heroes:server:production",
    "serve:ssr": "concurrently --names 'JSONDB,SERVER' --prefix-colors 'blue,yellow,' --kill-others \"npm:serve:database\" \"node dist/tour-of-heroes/server/main.js --configuration production\"",
    "prod:ssr": "npm run build:ssr && npm run serve:ssr",
    "postbuild:ssr": "ts-node tools/scripts/postbuild.ts",
    "prepare": "husky install",
    "prerender": "nx run tour-of-heroes:prerender",
    "postinstall": "node ./tools/scripts/decorate-angular-cli.js",
    "workspace-generator": "nx workspace-generator"
  },
  "sideEffects": [
    "**/main.ts",
    "**/main.server.ts",
    "**/polyfills.ts"
  ],
